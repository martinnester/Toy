//basic print statement
print 42;

//print complex expressions
print 3 * 5;

//print a string
print "Hello world!";

//print a concatenated string
print "Hello" .. "world!";

//print with escaped characters
print "\tHello\nworld";

//print from a leaf string
print "Hello world"[0,5];

print ("hello" .. "world")[2,6];
